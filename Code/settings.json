{
    "files.autoSave": "onFocusChange",
    "editor.renderWhitespace": "all",
    "editor.fontFamily": "\"JetBrains Mono\", monospace",
    // "editor.accessibilitySupport": "off",
    // "editor.fontFamily": "FiraCode-VF.ttf",
    "editor.fontLigatures": true,
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": false,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": false,
        "suppressShowKeyBindingsNotice": true,
        "suppressUpdateNotice": true,
        "suppressWelcomeNotice": false
    },
    "editor.wordWrap": "on",
    "extensions.ignoreRecommendations": true,
    "gitlens.keymap": "alternate",
    "gitlens.historyExplorer.enabled": true,
    "files.exclude": {
        "**/__generated__": true,
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.hg": true,
        "**/.next": true,
        "**/.svn": true,
        "**/*.aux": true,
        "**/*.fdb_latexmk": true,
        "**/*.fls": true,
        "**/*.out": true,
        "**/*.pyc": true,
        "**/*.xdv": true,
        "**/CVS": true
    },
    "editor.formatOnSave": false,
    "workbench.startupEditor": "welcomePage",
    "languageServerHaskell.useCustomHieWrapper": true,
    "languageServerHaskell.useCustomHieWrapperPath": "hie",
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true,
    "editor.tabSize": 2,
    "latex-workshop.view.pdf.viewer": "tab",
    "workbench.colorTheme": "Visual Studio Dark",
    "workbench.editor.enablePreview": false,
    "editor.mouseWheelZoom": true,
    "security.workspace.trust.untrustedFiles": "open",
    "npm.keybindingsChangedWarningShown": true,
    "codetogether.userName": "srghma",
    "[python]": {
        "editor.formatOnType": true
    },
    "cSpell.userWords": [
        "tinymce"
    ],
    "editor.formatOnPaste": true,
    "tabby.api.endpoint": "http://47.103.63.15:50085/",
    "tabby.keybindings": "tabby-style",
    "tabby.codeCompletion": true,
    "bitoAI.codeCompletion.enableAutoCompletion": true,
    "bitoAI.codeCompletion.enableCommentToCode": true,
    "editor.inlineSuggest.showToolbar": "always",
    "bitoAI.appearance.fontSize (Match with IDE Font)": false,
    "files.associations": {
        "*.bkp": "latex"
    },
    "purescript.addNpmPath": true,
    "haskell.manageHLS": "PATH",
}
